package hmh.sap.rfc.wrapper;

import hmh.sap.rfc.ZrestrictionType;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement(name = "ZrestrictionType")
public class ZrestrictionType_Wrapper {

		private ZrestrictionType instance;
		
		public void setInstance(ZrestrictionType instance) {
			this.instance = instance;
		}
		
		public ZrestrictionType_Wrapper() {}
		
		public ZrestrictionType_Wrapper(ZrestrictionType instance) {
			this.instance = instance;
		}

		
		//
		//	delegate methods
		//

		public void setMatnr(String Matnr) {
			instance.setMatnr(Matnr);
		}

		public String getRestricted() {
			return instance.getRestricted();
		}

		public void setRestricted(String Restricted) {
			instance.setRestricted(Restricted);
		}

		public String getMatnr() {
			return instance.getMatnr();
		}

}
